    "dev": "vite"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.10",
    "chokidar": "^4.0.3",
    "glob": "^11.0.3",
    "liquidjs": "^10.21.1",
    "sass": "^1.89.2",
    "tailwindcss": "^4.1.10",
    "vite": "^5.3.5"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
