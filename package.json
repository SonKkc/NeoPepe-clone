{
  "name": "theme-mab-io",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "watch:sass": "sass --watch assets/scss/main.scss assets/css/build.css",
    "watch:css": "npx @tailwindcss/cli -i assets/css/tailwind.css -o assets/css/styles.min.css --minify --watch",
